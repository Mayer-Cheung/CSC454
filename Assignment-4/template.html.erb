<html>
	<head>
		<link rel = "stylesheet" type = "text/css" href = "css/style.css">
		<link rel = "stylesheet" type = "text/css" href = "css/highlighter.css">

		<script src = "js/highlighter.js"></script>
		<script>
			function load(){
				highlight();
			}
		</script>
	</head>
	<body onload = "load()">
		<h1 id = "title">Cross-Indexing</h1>
		<div class = "code">
			<div class = "assembly">
				<% final_arr.each do |final_tuple| %>
					<% if final_tuple.instr == nil%>
						<% if final_tuple.pc != nil %>
							<span><%= final_tuple.pc %></span></br>
						<% else %>
							</br>
						<% end %>
					<% else %>
						<span id = "<%= final_tuple.pc %>">
							<% if final_tuple.target != nil %>
								<a href = "#<%= final_tuple.target %>"><%= final_tuple.instr %></a></br>
							<% else %>
								<%= final_tuple.instr %></br>
							<% end %>
						</span>
					<% end %>
				<% end %>
            </div>
            <div class = "source">
				<% final_arr.each do |final_tuple| %>
					<% if final_tuple.src == nil%>
						</br>
					<% else %>
						<% if final_tuple.gray %>
							<span class = "srcline"><span class = "gray"><%= final_tuple.src %></span></span></br>
						<% else %>
							<span class = "srcline"><%= final_tuple.src %></span></br>
						<% end %>
					<% end %>
				<% end %>
			</div>
		</div>
	<body>
</html>
